{{!< default}}

{{#post}}
  <main id="ub-static" class="content container" role="main">
    <div class="col-md-12">
      <article class="panel panel-default {{post-class}}">
        {{! Post content }}
        <section class="panel-body post-excerpt">
          {{content}}
        </section>
      </article>
    </div>
  </main>

{{/post}}